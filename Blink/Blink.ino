#include <stdio.h>
#include <stdlib.h>

  /***************/
 /** CONSTANTS **/
/***************/

int LED_PIN = 13;

//=======================================================================================================================================================

  /***********/
 /** SETUP **/
/***********/

void setup(){
  pinMode(LED_PIN , OUTPUT);
}

  /***********/
 /** LOOP ***/
/***********/

void loop()
{
  digitalWrite(LED_PIN , !digitalRead(LED_PIN));
  delay(500);
}
